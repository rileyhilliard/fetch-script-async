{
  "name": "fetch-script-async",
  "version": "1.0.1",
  "description": "async script fetcher to help lazy load scripts after a document has loaded",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rileyhilliard/fetch-script-async.git"
  },
  "keywords": [
    "async",
    "script",
    "fetch",
    "lazy-load",
    "lazy",
    "load"
  ],
  "author": "Riley Hilliard <rileyhilliard@gmail.com> (http://rileyh.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rileyhilliard/fetch-script-async/issues"
  },
  "homepage": "https://github.com/rileyhilliard/fetch-script-async",
  "scripts": {
    "push": "yarn build && yarn publish",
    "publish": "git add . && git commit -am \"Autogenerated pre-deployment commit\" ; npm version ${BUMP:-\"patch\"} --no-git-tag-version && git add package.json && git commit -m \"Autogenerated pre-deployment commit\" && bower version ${BUMP:-\"patch\"} && git push --tags && git push -u origin HEAD && npm publish",
    "build": "webpack --env dev && webpack --env build && npm run test",
    "start": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/fetch-script-async.js)\""
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "mock-local-storage": "^1.1.8",
    "nyc": "^13.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
